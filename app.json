{
  "expo": {
    "name": "ReactAssignment3",
    "slug": "ReactAssignment3",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/soundBoard.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/soundBoard.png",
      "resizeMode": "native",
      "backgroundColor": "#191919"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/soundBoard.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/soundBoardFavicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "router"
  }
}
